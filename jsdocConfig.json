{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "sourceType": "module",
  "source": {
    "include": [
      "./src/"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionnaries": [
      "jsdoc",
      "closure"
    ]
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  }
}